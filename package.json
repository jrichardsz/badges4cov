  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "mocha": "8.1.3",
    "mocha-suppress-logs": "^0.3.1",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "args-parser": "^1.3.0",
    "badgs": "^1.1.1",
    "jsonpath": "^1.1.1"
  }
}
